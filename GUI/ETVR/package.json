{
  "name": "etvr",
  "version": "1.0.0",
  "private": true,
  "description": "A SolidJS project for EyeTrack VR",
  "author": "EyeTrack VR",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite start",
    "serve": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:dev:release": "tauri dev --release",
    "tauri:build": "tauri build",
    "tauri:build:dev": "tauri build --debug",
    "tauri:update": "cd src-tauri && cargo update && cargo upgrade",
    "tauri:clean": "cd src-tauri && cargo clean",
    "backend:build": "cd ETVR-backend && make pyinstaller",
    "docs": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "format": "pnpm run lint --fix & pnpm prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@tailwindcss/forms": "^0.5.6",
    "@tauri-apps/api": "^1.4.0",
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "better-docs": "^2.7.2",
    "eslint": "^8.48.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "https-localhost": "^4.7.1",
    "jsdoc": "^4.0.2",
    "postcss": "^8.4.29",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0"
  },
  "dependencies": {
    "@kobalte/core": "^0.11.0",
    "@kobalte/tailwindcss": "^0.8.0",
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@solid-primitives/i18n": "^1.4.1",
    "@solid-primitives/map": "^0.4.8",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@stitches/core": "^1.2.8",
    "@tailwindcss/typography": "^0.5.10",
    "babel-preset-solid": "^1.7.7",
    "clsx": "^2.0.0",
    "lodash": "^4.17.21",
    "solid-form-handler": "^1.2.0",
    "solid-headless": "^0.13.1",
    "solid-i18n": "^1.1.0",
    "solid-icons": "^1.0.11",
    "solid-js": "^1.7.11",
    "solid-spinner": "^0.1.9",
    "solid-transition-group": "^0.2.3",
    "solidjs-icons": "^1.0.12",
    "solidjs-use": "^2.3.0",
    "tauri-plugin-log-api": "github:tauri-apps/tauri-plugin-log",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store",
    "tauri-plugin-upload-api": "github:tauri-apps/tauri-plugin-upload",
    "undici": "^5.23.0",
    "yup": "^1.2.0"
  }
}
